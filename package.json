{
  "name": "raxee-bot",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "compile": "tsc",
    "lint": "eslint .",
    "lint:ci": "DEBUG=eslint:eslint eslint .",
    "prettier": "prettier --write '**/*.(ts|js|md|json)'",
    "prettier:ci": "prettier --check '**/*.(ts|js|md|json)'",
    "start": "node --loader ts-node/esm -r dotenv/config bin/bot.ts",
    "start:prod": "prisma migrate deploy && npm run start",
    "test": "vitest run --coverage --coverage.include='**/*.ts'",
    "test:ci": "prisma generate && vitest run",
    "test:codecov": "prisma generate && npx vitest run --coverage --coverage.include='**/*.ts'"
  },
  "dependencies": {
    "@grammyjs/auto-retry": "^2.0.2",
    "@grammyjs/runner": "^2.0.3",
    "@prisma/client": "^6.19.1",
    "@types/node": "^22.19.3",
    "@vlad-yakovlev/grammy-reply-with-markdown": "^2.0.1",
    "@vlad-yakovlev/poker": "^1.4.0",
    "@vlad-yakovlev/telegram-md": "^2.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "grammy": "^1.38.4",
    "remeda": "^2.32.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vlad-iakovlev/eslint-config": "^1.0.0",
    "eslint": "^9.39.2",
    "jiti": "^2.6.1",
    "lefthook": "^2.0.12",
    "prettier": "^3.7.4",
    "prettier-plugin-packagejson": "^2.5.20",
    "prisma": "^6.19.1",
    "vitest": "^4.0.3",
    "vitest-mock-extended": "^3.1.0"
  }
}
