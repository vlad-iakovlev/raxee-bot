{
  "name": "raxee-bot",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm -r dotenv/config bin/bot.ts",
    "start:prod": "prisma migrate deploy && npm run start",
    "compile": "tsc",
    "lint": "eslint .",
    "lint:ci": "DEBUG=eslint:eslint eslint .",
    "prettier": "prettier --write '**/*.(ts|js|md|json)'",
    "prettier:ci": "prettier --check '**/*.(ts|js|md|json)'",
    "test": "jest --coverage",
    "test:ci": "prisma generate && jest"
  },
  "dependencies": {
    "@grammyjs/auto-retry": "^2.0.2",
    "@grammyjs/runner": "^2.0.3",
    "@prisma/client": "^6.16.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.18.6",
    "@vlad-yakovlev/grammy-reply-with-markdown": "^2.0.1",
    "@vlad-yakovlev/poker": "^1.4.0",
    "@vlad-yakovlev/telegram-md": "^2.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "grammy": "^1.38.2",
    "remeda": "^2.32.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.39",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.2.0",
    "jiti": "^2.6.0",
    "lefthook": "^1.13.4",
    "prettier": "^3.6.2",
    "prisma": "^6.16.2",
    "typescript-eslint": "^8.44.1"
  }
}
